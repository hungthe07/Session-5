1.	CREATE TABLE KhachHang (
2.	    MaKH INT,
3.	    HoTen VARCHAR(255),
4.	    Email VARCHAR(255),
5.	    SoDienThoai VARCHAR(15),
6.	    NgayDangKy DATE
7.	);
